<!DOCTYPE html>
<html>
<head>
	<title> <%= title %> </title>
    <%= render 'layouts/baju' %>
</head>
<body>
	<div class= "container">
		<%= render 'layouts/header' %>
		<section>
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>">
					<%= value %>
				</div>
			<% end %>
			<%= yield %>
		</section>    	
    	<%= debug(params) if Rails.env.development? %>
	</div>
</body>
</html>
